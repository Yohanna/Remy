{
  "name": "remy",
  "version": "0.0.1",
  "private": true,
  "description": "Remy Backend API Server",
  "main": "app.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "nodemon --exec ts-node app.ts",
    "debug": "export DEBUG=swagger-tools:middleware:* || set DEBUG=swagger-tools:middleware:*&& npm start",
    "build": "tsc && npm run copy-files",
    "build:w": "tsc -w && npm run copy-files",
    "copy-files": "copyfiles ./config/default.yaml ./dist/ && copyfiles ./api/swagger/* ./dist/",
    "test": "swagger project test",
    "lint": "eslint ./dist"
  },
  "dependencies": {
    "@google/maps": "^0.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.12.3",
    "geolib": "2.0.22",
    "google-locations": "0.2.0",
    "helmet": "^3.1.0",
    "morgan": "^1.8.1",
    "mysql": "^2.13.0",
    "pug": "^2.0.0-beta11",
    "swagger": "^0.7.5",
    "swagger-express-mw": "0.1.0",
    "swagger-tools": "^0.10.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/mysql": "0.0.31",
    "@types/node": "^7.0.10",
    "copyfiles": "^1.2.0",
    "eslint": "^3.10.2",
    "nodemon": "1.11.0",
    "should": "^8.4.0",
    "supertest": "^1.2.0",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.1"
  }
}
